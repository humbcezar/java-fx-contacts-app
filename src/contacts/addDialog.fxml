<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<DialogPane fx:controller="contacts.DialogController" xmlns:fx="http://javafx.com/fxml" >
    <content>
        <GridPane>
            <Label text="First Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="firstName" GridPane.rowIndex="1" GridPane.columnIndex="0"/>

            <Label text="Last Name" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="lastName" GridPane.rowIndex="3" GridPane.columnIndex="0"/>

            <Label text="Phone Number" GridPane.rowIndex="4" GridPane.columnIndex="0"/>
            <TextField  fx:id="phoneNumber" GridPane.rowIndex="5" GridPane.columnIndex="0"/>

            <Label text="Notes" GridPane.rowIndex="6" GridPane.columnIndex="0"/>
            <TextField fx:id="notes" GridPane.rowIndex="7" GridPane.columnIndex="0"/>
        </GridPane>
    </content>
</DialogPane>